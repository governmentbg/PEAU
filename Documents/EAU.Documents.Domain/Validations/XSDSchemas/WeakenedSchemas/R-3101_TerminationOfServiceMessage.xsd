<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/segment/R-3101" xmlns="http://ereg.egov.bg/segment/R-3101" xmlns:drsd="http://ereg.egov.bg/value/0008-000004" 
