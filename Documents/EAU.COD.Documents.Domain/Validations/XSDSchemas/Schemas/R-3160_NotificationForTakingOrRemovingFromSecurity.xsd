<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/segment/R-3160" xmlns="http://ereg.egov.bg/segment/R-3160" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:eash="http://ereg.egov.bg/segment/0009-000152"
            xmlns:stbt="http://ereg.egov.bg/value/0008-000143"
            xmlns:sard="http://ereg.egov.bg/segment/0009-000141"
            xmlns:nftorfsd="http://ereg.egov.bg/segment/R-3161"
            xmlns:sod="http://ereg.egov.bg/segment/R-3162"
            xmlns:dm="http://ereg.egov.bg/segment//R-3136"
            xmlns:ad="http://ereg.egov.bg/segment/0009-000139"
            xmlns:easf="http://ereg.egov.bg/segment/0009-000153"
            elementFormDefault="qualified">
  <xsd:import namespace="http://ereg.egov.bg/segment/0009-000152" />
  <xsd:import namespace="http://ereg.egov.bg/value/0008-000143"/>
  <xsd:import namespace="http://ereg.egov.bg/segment/0009-000141" />
  <xsd:import namespace="http://ereg.egov.bg/segment/R-3161" />
  <xsd:import namespace="http://ereg.egov.bg/segment/R-3162" />
  <xsd:import namespace="http://ereg.egov.bg/segment//R-3136" />
  <xsd:import namespace="http://ereg.egov.bg/segment/0009-000139" />
  <xsd:import namespace="http://ereg.egov.bg/segment/0009-000153" />
  <xsd:element name="NotificationForTakingOrRemovingFromSecurity" type="NotificationForTakingOrRemovingFromSecurity" />
  <xsd:complexType name="NotificationForTakingOrRemovingFromSecurity">
    <xsd:annotation>
      <xsd:documentation xml:lang="bg">Уведомление за поемане или снемане от охрана на обект при извършване на частна охранителна дейност</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ElectronicAdministrativeServiceHeader" type="eash:ElectronicAdministrativeServiceHeader"  minOccurs="0" />
      <xsd:element name="ServiceTermType" type="stbt:ServiceTermType" minOccurs="0" />
      <xsd:element name="ServiceApplicantReceiptData" type="sard:ServiceApplicantReceiptData"  minOccurs="0" />
      <xsd:element name="NotificationForTakingOrRemovingFromSecurityData" type="nftorfsd:NotificationForTakingOrRemovingFromSecurityData"  />
      <xsd:element name="SecurityObjectsData" type="sod:SecurityObjectsData"  />
      <xsd:element name="Declarations" minOccurs="0">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Declaration" type="dm:Declaration" maxOccurs="unbounded" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="AttachedDocuments" minOccurs="0">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="AttachedDocument" type="ad:AttachedDocument" maxOccurs="unbounded" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ElectronicAdministrativeServiceFooter" type="easf:ElectronicAdministrativeServiceFooter"  minOccurs="0" />
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>