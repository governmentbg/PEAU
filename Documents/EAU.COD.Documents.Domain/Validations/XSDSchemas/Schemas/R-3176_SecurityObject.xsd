<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/segment/R-3176" xmlns="http://ereg.egov.bg/segment/R-3176" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
