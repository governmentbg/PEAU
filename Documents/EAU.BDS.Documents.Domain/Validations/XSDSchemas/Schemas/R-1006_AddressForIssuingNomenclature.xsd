<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/nomenclature/R-1006" xmlns="http://ereg.egov.bg/nomenclature/R-1006" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xsd:simpleType name="AddressForIssuingNomenclature">
    <xsd:annotation>
      <xsd:documentation xml:lang="bg">Номенклатура за изпълняване на заявление по настоящ/постоянен адрес</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <!-- По постоянен адрес -->
      <xsd:enumeration value="1" />
      <!-- По настоящ адрес -->
      <xsd:enumeration value="2" />
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>