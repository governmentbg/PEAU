            xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

	<xsd:simpleType name="WeaponNoticeTypeNomenclature">
		<xsd:annotation>
			<xsd:documentation xml:lang="bg">Номеклатура за типа на уведомплението при оръжие</xsd:documentation>
